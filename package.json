{
  "name": "@joseph-leedy/express-magento2-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "ExpressJS Magento2 API client app",
  "author": "Joseph Leedy",
  "license": "MIT",
  "main": "main.ts",
  "scripts": {
    "dev": "tsx src/main.ts",
    "dev:watch": "tsx --watch src/main.ts",
    "dev:debug": "tsx --inspect-brk src/main.ts",
    "build": "swc src -d dist",
    "build:watch": "swc src -d dist -w",
    "lint": "eslint src",
    "test": "PORT=3001 NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:coverage": "PORT=3001 NODE_OPTIONS=--experimental-vm-modules jest --runInBand --coverage",
    "start": "NODE_ENV=production node dist/main.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@stryker-mutator/jest-runner": "^8.2.2",
    "@swc/cli": "^0.3.5",
    "@swc/core": "^1.3.107",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.10",
    "@types/supertest": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JosephLeedy/express-magento2-client"
  },
  "keywords": [
    "magento",
    "adobe",
    "commerce"
  ],
  "bugs": {
    "url": "https://github.com/JosephLeedy/express-magento2-client/issues"
  },
  "homepage": "https://github.com/JosephLeedy/express-magento2-client#readme",
  "engines": {
    "node": ">=18.0.0 < 22.0.0",
    "npm": "^10"
  }
}
